#include<stdio.h>
int main()
{
	int n,a=0, b=1,c,sum=0,total=0;
	printf("enter the number:  ");
	scanf("%d",&n);
	printf("%d\n",a);
	printf("%d\n",b);
	sum=a+b;
	for(int i=1; i<n; i++)
	{
		c=a+b;
		printf("%d\n",c);
		a=b;
		b=c;
		total += c;
	}
	total+=sum;
	printf("sum: %d",total);
	return 0;
}
