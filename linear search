#include <stdio.h>
int main()
{
    int i,n,a[50],key,flag=0;
    printf("enter the n value:");
    scanf("%d",&n);
    printf("enter the elements into the array:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the key value:");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(a[i]==key)
        {
            flag=1;
            break;
        }
    }
    if(flag==1)
    {
        printf("the key element is found in the array");
    }
    else
    {
        printf("the key elemnt is not found in the array");
    }
    return 0;
}
